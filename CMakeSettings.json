{
  "configurations": [
    {
      "name": "x64-Debug",
      "generator": "Ninja",
      "configurationType": "Debug",
      "inheritEnvironments": [ "msvc_x64_x64" ],
      "buildRoot": "${projectDir}\\build\\debug",
      "installRoot": "${projectDir}\\install",
      "cmakeCommandArgs": "",
      "buildCommandArgs": "",
      "ctestCommandArgs": "",
      "variables": [
        {
          "name": "CMAKE_TOOLCHAIN_FILE",
          "value": "${projectDir}\\vcpkg\\scripts\\buildsystems\\vcpkg.cmake"
        }
      ],
      "launch": [
        {
          "project": "Engine",
          "projectTarget": "Engine.exe",
          "name": "Engine Debug",
          "type": "default",
          "debuggerWorkingDirectory": "${projectDir}"
        }
      ]
    }
  ]
}
